{
    "manifest_version": 3,
    "name": "Confidence Checker",
    "version": "1.0",
    "description": "Helps you reflect before applying to tech jobs",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "scripting",
      "activeTab"
    ],
    "host_permissions": [
      "*://*/*"
    ]
  }
  